{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "test-api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test-api",
      "dest": "/test-api"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
